{

  "extends": "../../tsconfig.json",
  "vueCompilerOptions": {
    "plugins": [
      "@vue-macros/volar/define-model",
      "@vue-macros/volar/define-slots"
    ]
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "~types/*": [
        "../types/*"
      ]
    }
  },
  "exclude": ["dist", "node_modules", "cypress"]
}
